<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Welcome to the-internet" parallel="tests">
    <parameter name="env" value="herokuapp"/>
    
    <test name="Context Menu">
        <classes>
            <class name="Lecture_12_13.HomePage_Test"/>
            <class name="Lecture_12_13.ContextMenu_Test"/>
        </classes>
    </test>

    <test name="Dynamic Controls">
        <classes>
            <class name="Lecture_12_13.HomePage_Test"/>
            <class name="Lecture_12_13.DynamicControls_Test"/>
        </classes>
    </test>

    <test name="File Uploader" enabled="false">
        <classes>
            <class name="Lecture_12_13.HomePage_Test"/>
            <class name="Lecture_12_13.FileUploader_Test"/>
        </classes>
    </test>

    <test name="Frames">
        <classes>
            <class name="Lecture_12_13.HomePage_Test"/>
             <class name="Lecture_12_13.Frames_Test"/>
        </classes>
    </test>
</suite>